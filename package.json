{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "npx vite build",
    "lint": "eslint .",
    "preview": "npx vite preview",
    "build:windows": "npm run build && electron-builder --win",
    "build:android": "npm run build && capacitor build android",
    "setup:mobile": "npm install @capacitor/core @capacitor/cli @capacitor/android && npx cap init",
    "setup:desktop": "npm install electron electron-builder --save-dev"
  },
  "dependencies": {
    "dependencies": "^0.0.1",
    "lucide-react": "^0.344.0",
    "openai": "^5.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@supabase/supabase-js": "^2.39.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "@capacitor/core": "^5.6.0",
    "@capacitor/cli": "^5.6.0",
    "@capacitor/android": "^5.6.0"
  },
  "build": {
    "appId": "com.quadrax.ml",
    "productName": "QUADRAX•ML",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "capacitor": {
    "appId": "com.quadrax.ml",
    "appName": "QUADRAX•ML",
    "webDir": "dist",
    "bundledWebRuntime": false
  }
}